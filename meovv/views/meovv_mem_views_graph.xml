<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="meovv_mem_view_graph" model="ir.ui.view">
            <field name="name">meovv.mem.graph</field>
            <field name="model">meovv.mem</field>
            <field name="arch" type="xml">
                <graph type="bar">
                    <field name="apply_job"/>
                    <field name="name"/>
                    <field name="dob" interval="year" invisible="1"/>
                    <field name="salary" type="measure" string="exp salary"/>

                </graph>
            </field>
        </record>
    </data>
</odoo>