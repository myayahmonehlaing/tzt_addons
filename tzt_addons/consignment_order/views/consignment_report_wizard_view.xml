<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="view_consignment_report_wizard_form" model="ir.ui.view">
        <field name="name">consignment.report.wizard.form</field>
        <field name="model">consignment.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Consignment Report Wizard">
                <group>
                    <field name="date_to"/>
                </group>
                <footer>
                    <button name="action_print_pdf" type="object" string="Report PDF" class="btn-primary"/>
                    <button name="action_print_excel" type="object" string="Report Excel" class="btn-secondary"/>
                    <button name="action_show_pivot" type="object" string="Consignment Pivot" class="btn-secondary"/>
                    <button string="Cancel" class="btn-link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>